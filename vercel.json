{
  "buildCommand": "cd front-end && npm install && npm run build",
  "outputDirectory": "front-end/build",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/app.js"
    },
    {"source": "/(.*)", "destination": "/"},
    {"source": "/admin/(.*)", "destination": "/backoffice/$1"}
  ]
}